#########################root################################
log4j.rootLogger=DEBUG,seobi

########################appender#############################
log4j.appender.seobi=org.apache.log4j.ConsoleAppender
log4j.appender.seobi.layout=org.apache.log4j.PatternLayout
log4j.appender.seobi.layout.ConversionPattern= time[%d{yyyy-MM-dd HH:mm:ss}] leve[%p] msg[%m] %x class[%c%l] thread[%t] %n

